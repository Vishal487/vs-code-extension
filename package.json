{
    "name": "code-snippets",
    "displayName": "Code Sniper",
    "description": "code snippets for python/c/c++",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "publisher": "VishalRao",
    "author": {
        "name": "Vishal Rao"
    },
    "homepage": "https://github.com/Vishal487/vs-code-extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/Vishal487/vs-code-extension"
    },
    "icon": "images/logo.png",
    "keywords": [
        "python",
        "c/cpp",
        "python code snippets",
        "c/cpp code snippets",
